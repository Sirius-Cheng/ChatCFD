FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      CH4;
}

dimensions      [0 0 0 0 0 0 0];
internalField   uniform 0;
boundaryField
{
    walls
    {
        type            fixedValue;
        value           uniform 0;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    outlet
    {
        type            zeroGradient;
    }
}
