FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}

dimensions      [0 1 -1 0 0 0 0];
internalField   uniform (<U_x> <U_y> <U_z>);
boundaryField
{
    velocity_inlet_air
    {
        type            fixedValue;
        value           uniform (<U_inlet_air_x> <U_inlet_air_y> <U_inlet_air_z>);
    }
    velocity_inlet_nh3
    {
        type            fixedValue;
        value           uniform (<U_inlet_NH3_x> <U_inlet_NH3_y> <U_inlet_NH3_z>);
    }
    pressure_outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            noSlip;
    }
}
