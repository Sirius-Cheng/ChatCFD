FoamFile
{
    version     2.0;
    format      ascii;
    class       fvSchemes;
    object      fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

divSchemes
{
    default         none;
    div(rho*phi,U)  boundedGaussSeidel;
    div(phi,U)      boundedGaussSeidel;
    div(rho*phi,nuTilda) boundedGaussSeidel;
}

gradSchemes
{
    default         Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         gaussGrad;
}