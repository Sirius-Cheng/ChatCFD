divSchemes
{
    default         none;
    div(phi,U)      bounded;
    div(phi,k)      bounded;
    div(phi,omega)  bounded;
    div(phi,nuTilda) bounded;
    div(phi,Y)      bounded;
    div(phi,alpha)  bounded;
}

