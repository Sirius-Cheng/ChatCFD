FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          GAMG;
        smoother        DICGaussSeidel;
        tolerance       1e-06;
        relTol          0.05;
        minIter         1;
    }

    pFinal
    {
        $p;
        smoother        DICGaussSeidel;
        tolerance       1e-06;
        relTol          0.01;
    }

    " (U|k|omega|nuTilda) "
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-05;
        relTol          0.1;
        minIter         1;
    }

    " (U|k|omega|nuTilda)Final "
    {
        $U;
        relTol          0;
    }
}

PIMPLE
{
    nOuterCorrectors             3;
    nCorrectors                  2;
    nNonOrthogonalCorrectors    0;
    pRefCell                     1000;
    pRefValue                    0;
    momentumPredictor            on;
}

relaxationFactors
{
    fields
    {
        "(p|pFinal)"           0.3;
    }
    equations
    {
        "(U|UFinal)"           0.7;
        "(k|kFinal)"           0.7;
        "(omega|omegaFinal)"   0.7;
        "(nuTilda|nuTildaFinal)" 0.7;
    }
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //