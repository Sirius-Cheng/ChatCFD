FoamFile  
{  
    version     2.0;  
    format      ascii;  
    class       fvSchemes;  
    object      fvSchemes;  
}  

divSchemes  
{  
    default         none;  
    div(phi,U)      Gauss limitedLinearV 1;  
    div(phi,Y)      Gauss limitedLinear 1;  
    div(phi,alphat) Gauss limitedLinear 1;  
}  

gradSchemes  
{  
    default         Gauss linear;  
}  

laplacianSchemes  
{  
    default         Gauss linear corrected;  
}  

interpolationSchemes  
{  
    default         linear;  
}  

snGradSchemes  
{  
    default         orthogonal;  
}